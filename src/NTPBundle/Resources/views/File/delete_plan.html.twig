{% extends 'NTPBundle::base.html.twig' %}
{% block body %}
        <br/>
        <div class="row">
            <div class="col-md-4">
                <div class="row">
                    <div class="col-md-5">
                        {{ form_start(form,{'attr': {'id': 'delete_paln'}}) }}
                        {{ form_errors(form) }}
