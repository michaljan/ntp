{% extends 'NTPBundle::base.html.twig' %}
{% block body %}
<table class="table table-condensed">
    {% for item in fileProcessed %}
        <tr>
        <td>{{ item.getName }}</td>
        <td>{{ item.getPath }}</td>
        <td>{{ item.getUploadedAt|date('d-m-Y H:i') }}</td>
        <td>
            <a href="{{ path('process_csv',{ id: item.id }) }}" class="btn btn-success">Process</a>    
        </td>
        </tr>
    {% endfor %}
        
</table>
{% endblock %}
